@inherits LayoutComponentBase

<MudThemeProvider />
<MudDialogProvider />
<MudSnackbarProvider />

<MudLayout>
    <MudAppBar Elevation="1">
        <MudIconButton Icon="@Icons.Material.Filled.Menu" Color="Color.Inherit" Edge="Edge.Start" OnClick="@ToggleDrawer" />
        <MudSpacer />
        <MudIconButton Icon="@Icons.Custom.Brands.GitHub" Color="Color.Inherit" Link="https://github.com/MudBlazor/MudBlazor" Target="_blank" />
    </MudAppBar>
    <MudDrawer @bind-Open="@open" Elevation="1">
        <MudDrawerHeader>
            <MudText Typo="Typo.h6">Progressive Overlord</MudText>
        </MudDrawerHeader>
        <MudNavMenu Rounded="true" Color="Color.Tertiary">
            <MudNavLink Href="/jennitesting" Match="NavLinkMatch.All">Jenni's Testing</MudNavLink>
            <MudNavLink Href="/VTesting" Match="NavLinkMatch.All">OnlyFans</MudNavLink>
            <MudNavLink Match="NavLinkMatch.All">FansOnly</MudNavLink>
        </MudNavMenu>
    </MudDrawer>
    <MudMainContent Class="pt-16 px-16">
        <MudContainer Class="mt-6">
            <WorkoutApp.Pages.JenniTesting></WorkoutApp.Pages.JenniTesting>
        </MudContainer>
    </MudMainContent>
</MudLayout>



